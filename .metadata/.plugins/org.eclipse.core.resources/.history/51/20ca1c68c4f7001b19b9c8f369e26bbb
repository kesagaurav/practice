import java.util.Arrays;
import java.util.Comparator;

public class PracticeMain {

	public static void main(String[] args) {
	
		int A[]= {4,8,2};
		int b[]= {10,16,6};
		int c=0;
		
		int n=3;
		
		for(int i=0;i<n;i++) {
			  c=b[i]/A[i];
			
			}
		 
		System.out.println(Arrays.asList(c).stream().mapToInt(v->v).max());
	
		
	
	}

	
		
}
