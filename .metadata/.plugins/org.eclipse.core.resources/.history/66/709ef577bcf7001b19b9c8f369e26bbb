import java.util.ArrayList;
import java.util.List;

public class Main {

	public static void main(String[] args) {
		List<Integer> li=new ArrayList<>();
		li.add(1);
		li.add(null);
		li.add(2);
		li.add(3);
		for(int i=0;i<li.size();i++) {
			li.stream().forEach(e->System.out.println(e));
		}

	}

}
