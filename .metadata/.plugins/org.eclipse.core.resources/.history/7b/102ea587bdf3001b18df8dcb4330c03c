import java.util.ArrayList;
import java.util.List;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		List li = new ArrayList<>();
		li.add("hi");
		li.add(12.0f);
		li.add(true);
		System.out.println(" li" + li);

		List<String> a=new ArrayList<>();
		a.add("gaurav");
		a.add("surya");
		a.add("sdnjdfnjkdf");
		a.add("ndfjnjksnjsk");
		a.add("ndfjnnsdknsdk");
		a.add(2,"deepa");
		a.addAll(a);
		a.get(0);
		System.out.println("a" + a);
		a.remove(2);
		System.out.println("a the removed element is" + a);

		for(int i=0;i<a.size();i++) {
			System.out.println( " " + a.get(i));
		}
	
		
		List<Integer> i=new ArrayList<>();
		i.add(1);
		i.add(2);
		i.add(3);
		i.add(4);
		i.add(5);
		for(int a1=0;a1<i.size();a1++) {
			if(a1%2==0) {
				System.out.println("even at " + i.get(a1));
			}else {
				System.out.println("odd" + i.get(a1));
			}
			System.out.println( " " + i.get(a1));
		}
	
	
	
	}

}
