import java.util.Arrays;
import java.util.Comparator;

public class PracticeMain {

	public static void main(String[] args) {
	
		int A[]= {4,8,2};
		int b[]= {10,16,6};
		
		int n=3;
		
		for(int i=0;i<3;i++) {
			int c=b[i]/A[i];
			System.out.println(c);
			if(c<A[i]) {
				c=A[i];
				System.out.println(c);
			}else {
				c=b[i];
				System.out.println(b);
			}
			
		}
	
	}

	
		
}
